{
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "ext-fileinfo": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "^3.10",
        "doctrine/migrations": "^3.9.1",
        "doctrine/orm": "^2.20.5",
        "fakerphp/faker": "^1.24.1",
        "itsgoingd/clockwork": "v5.1.12",
        "league/flysystem": "^3.30",
        "league/mime-type-detection": "^1.16",
        "mbezhanov/faker-provider-collection": "^2.0.2",
        "php-di/php-di": "^6.4",
        "slim/csrf": "^1.5",
        "slim/psr7": "^1.7.1",
        "slim/slim": "^4.14",
        "slim/twig-view": "^3.4.1",
        "symfony/cache": "^6.4.23",
        "symfony/console": "^6.4.23",
        "symfony/twig-bridge": "^6.4.22",
        "symfony/webpack-encore-bundle": "^1.17.2",
        "twig/intl-extra": "^3.21",
        "twig/twig": "^3.21.1",
        "vlucas/phpdotenv": "^5.6.2",
        "vlucas/valitron": "^1.4.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.23"
    }
}
